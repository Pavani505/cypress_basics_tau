{
  "name": "@applitools/core",
  "version": "4.14.0",
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/core-base"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./types": {
      "types": "./types/types.d.ts",
      "default": "./dist/types.js"
    },
    "./dist/*": "./dist/*.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "eyes": "./dist/cli/cli.js",
    "eyes-check-network": "./dist/troubleshoot/check-network.js"
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "browser": {
    "@applitools/ec-client": false
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.ts'",
    "build": "yarn build:dist",
    "build:dist": "run --top-level tsc --project ./tsconfig.build.json",
    "build:bin": "sea",
    "build:bin:zip": "zip -j ./bin/core.zip $(find ./bin -type f -not -name '*.zip' -not -name '*.tar.gz' | xargs)",
    "build:bin:tgz": "tar -czf ./bin/core.tar.gz $(find ./bin -type f -not -name '*.zip' -not -name '*.tar.gz' | xargs)",
    "build:full": "yarn build:dist && yarn build:bin",
    "test": "run --top-level mocha './test/**/*.spec.ts' --exclude './test/bin/**' --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "test:local": "MOCHA_OMIT_TAGS=sauce run test",
    "test:sauce": "MOCHA_ONLY_TAGS=sauce run test",
    "test:bin": "MOCHA_GROUP=bin run --top-level mocha './test/bin/**/*.spec.ts' --parallel --jobs ${MOCHA_JOBS:-15}",
    "test:e2e": "MOCHA_GROUP=e2e run --top-level mocha './test/e2e/**/*.spec.ts' --parallel --jobs ${MOCHA_JOBS:-15} --exit",
    "test:it": "MOCHA_GROUP=it run --top-level mocha './test/it/**/*.spec.ts'",
    "test:unit": "MOCHA_GROUP=unit run --top-level mocha './test/unit/**/*.spec.ts'",
    "setup": "run --top-level browsers:setup"
  },
  "sea": {
    "bin": "eyes",
    "certs": {
      "win": {
        "cert": "env.CODE_SIGNING_CERT_WIN",
        "pass": "env.CODE_SIGNING_PASS_WIN"
      }
    },
    "targets": {
      "node20.11-linux-x64-glibc-217": "./bin/core-linux",
      "node20-linux-x64-musl": "./bin/core-alpine",
      "node20-linux-arm64": "./bin/core-linux-arm64",
      "node20-darwin-x64": "./bin/core-macos",
      "node20-win-x64": "./bin/core-win.exe"
    }
  },
  "dependencies": {
    "@applitools/core-base": "1.13.0",
    "@applitools/dom-capture": "11.2.6",
    "@applitools/dom-snapshot": "4.11.1",
    "@applitools/driver": "1.17.2",
    "@applitools/ec-client": "1.8.4",
    "@applitools/logger": "2.0.16",
    "@applitools/nml-client": "1.8.4",
    "@applitools/req": "1.6.7",
    "@applitools/screenshoter": "3.8.31",
    "@applitools/snippets": "2.4.25",
    "@applitools/socket": "1.1.16",
    "@applitools/spec-driver-webdriver": "1.1.7",
    "@applitools/ufg-client": "1.11.2",
    "@applitools/utils": "1.7.2",
    "@types/ws": "8.5.5",
    "abort-controller": "3.0.0",
    "chalk": "4.1.2",
    "node-fetch": "2.6.7",
    "semver": "7.5.4",
    "webdriver": "7.31.1",
    "ws": "8.13.0",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@applitools/bongo": "^5.10.0",
    "@applitools/sea": "^1.0.0",
    "@applitools/spec-driver-puppeteer": "^1.4.7",
    "@applitools/spec-driver-selenium": "^1.5.78",
    "@applitools/test-server": "^1.2.2",
    "@applitools/test-utils": "^1.5.17",
    "@applitools/tunnel-client": "^1.5.2",
    "@types/node": "^12.20.55",
    "@types/selenium-webdriver": "^4.1.2",
    "@types/semver": "^7.5.4",
    "@types/yargs": "^17.0.22",
    "nock": "^13.3.2",
    "png-async": "^0.9.4",
    "puppeteer": "^19.11.1",
    "selenium-webdriver": "^4.15.0"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "publishConfig": {
    "access": "public"
  }
}